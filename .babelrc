{
  "presets": [
    "es2015",
    "react",
    "stage-1"
  ],
  "env": {
    "start": {
      "presets": [
        "react-hmre"
      ]
    }
  },
  "plugins": [["import", [
      {
        "libraryName": "antd",
        "style": true
      },
      {
        "libraryName": "material-ui",
        "libraryDirectory": "components",  // default: lib
        "camel2DashComponentName": false
      }
    ]]]
}
